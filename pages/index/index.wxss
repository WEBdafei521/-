/**index.wxss**/
.l-index-title>image{
  width: 100vw;
  height: 440rpx;
}
.l-main{
  position: relative;
}
.l-main-circle-one{
  position: absolute;
  width: 300rpx;
  height: 300rpx;
  border-radius: 100%;
  background: #fff;
  top: 50%;
  left: 50%;
  border: 0;
  transform: translateY(-50%) translateX(-50%);
}
.l-main-circle-two{
  width: 270rpx;
  height: 270rpx;
  border-radius: 100%;
  border: 0;
  background: rgba(200, 200, 200, .3);
  margin: 15rpx 0 0 15rpx;
  
}
.l-main-circle-three{
  width: 240rpx;
  height: 240rpx;
  border-radius: 100%;
  background: #fff;
  color: #fff;
  font-size: 36rpx;
  /* font-weight: 600; */
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.l-main-circle-four{
  width: 210rpx;
  height: 210rpx;
  font-size: 36rpx;
  /* font-weight: 600; */
  border-radius: 100%;
  background: rgba(200, 200, 200, .3);
  color: #000;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.l-main-top>view,.l-main-bottom>view{
  width: 43vw;
  height: 35vw;
  background: rgba(200, 200, 200, .2);
  margin-top: 36rpx;
  border-radius: 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.l-main-top-nei{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #000;
  font-size: 30rpx;
}